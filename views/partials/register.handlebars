<div class="signup">
  <div class="signUpHeader">
    <h2>Signup</h2>
  </div>
  <form class="signUpForm">
    <div>
      <div class="form-group">
        <label for="name-signup">name:</label>
        <input class="search" type="text" id="name-signup" />
      </div>
      <div class="form-group">
        <label for="email-signup">email:</label>
        <input class="search" type="text" id="email-signup" />
      </div>
      <div class="password">
        <label for="password-signup">password:</label>
        <input class="search" type="password" id="password-signup" />
      </div>
      <div class="form-group">
        <button class="btn btn-primary" type="submit" href="/logged">signup</button>
      </div>
  </form>
</div>