{{#each products.reviews as |review|}}
    <div class="userReviewFull">
    <div class="userInfoReview">
    <h2>Reviewed by: {{review.user.user_name}}</h2>
    <h2>On: {{review.date_created}}</h2>
    
    <div class="reviewDetails">
    <h2 class="ratingStar">Rating: {{review.stars}}</h2>
    </div>
    <h2 class="reviewText"id="productReviews" class="productReviews">{{review.text}}</h2>
    </div>
    </div>
{{/each}}