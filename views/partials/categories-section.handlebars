<div class="categories">
    {{#each categories as |category|}}
    <div>
        <h1>{{category.name}}</h1>
        {{#each category.products as |product|}}
        <img src="/assets/images/{{product.image}}">
        {{!-- <img src="/assets/images/monitors/samsung-odyssey-G9.jpg"> --}}
        <h2>{{product.name}}</h2>

        {{/each}}
    </div>
    {{/each}}

</div>